NOTE BEST PLAYER KEEPER
MAKE

COPY 0 X
MARK LOOP
  NOTE NAME
  COPY M F
  NOTE SCORE
  COPY M F
  TEST X = 0
  TJMP ASSIGN_AND_LOOP
  SEEK -1
  TEST F > X
  TJMP DELETE_PREV
  JUMP DELETE_CUR

MARK DELETE_PREV
  SEEK -1
  COPY F X
  SEEK -4
  VOID F
  VOID F
  SEEK 2
  JUMP LOOP

MARK DELETE_CUR
  SEEK -2
  VOID F
  VOID F
  JUMP LOOP

MARK ASSIGN_AND_LOOP
  SEEK -1
  COPY F X
  JUMP LOOP