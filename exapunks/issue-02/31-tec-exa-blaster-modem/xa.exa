GRAB 300
LINK 800

MARK DIAL
  SEEK -9999
  TEST EOF
  TJMP END
  COPY -1 #DIAL
  COPY 11 X
  MARK READ
    COPY F #DIAL
    SEEK -1
    VOID F
    SUBI X 1 X
    TEST X = 0
    FJMP READ

REPL SPAWN
SEEK 9999
NOOP

MARK COPY_PHONE
  TEST MRD
  FJMP DIAL
  COPY M X
  TEST X > -1
  FJMP COPY_PHONE
  COPY X F
  JUMP COPY_PHONE

MARK SPAWN
  LINK 800
  GRAB 200
  MARK READ_FILE
    COPY F M
    TEST EOF
    FJMP READ_FILE
    DROP
    
MARK END
  LINK -1